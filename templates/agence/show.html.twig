{% extends 'backbase.html.twig' %}

{% block title %}<title>Afficher Les Agences</title>{% endblock %}

{% block content %}
    <h1>Agence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agence.id }}</td>
            </tr>
            <tr>
                <th>Nom_agence</th>
                <td>{{ agence.nomAgence }}</td>
            </tr>
            <tr>
                <th>Email_agence</th>
                <td>{{ agence.emailAgence }}</td>
            </tr>
            <tr>
                <th>Tel_agence</th>
                <td>{{ agence.telAgence }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_agence_index') }}"class="btn btn-primary">back to list</a>

    <a href="{{ path('app_agence_edit', {'id': agence.id}) }}"class="btn btn-primary">edit</a>

    {{ include('agence/_delete_form.html.twig') }}
{% endblock %}
